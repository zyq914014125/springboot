<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- 配置驱动 -->
    <classPathEntry
            location="G:\javaEE+JAVASE\JavaWebSIMServelt（纯servelt管理界面）\JavaWebSIMServelt5\web\WEB-INF\lib\mysql-connector-java-5.1.47.jar"/>
    <context id="context1">
        <commentGenerator>
            <!-- 不希望生成的注释中包含时间戳 -->
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/mybits"
                        userId="root" password="123456789"/>
        <!-- 生成实体类 实体bean文件 -->
        <javaModelGenerator targetPackage="com.example.springboot.pojo" targetProject="src/main/java"/>
        <!-- mapper xml文件 -->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources"/>
        <!-- mapper 接口文件 -->
        <javaClientGenerator targetPackage="com.example.springboot.mapper" targetProject="src/main/java"
                             type="XMLMAPPER"/>

        <!-- schema为数据库名，oracle需要配置，mysql不需要配置。
         tableName为对应的数据库表名
         domainObjectName 是要生成的实体类名(可以不指定)
         enableXXXByExample 默认为 true， 为 true 会生成一个对应Example帮助类，帮助你进行条件查询，不想要可以设为false
         -->
        <table tableName="departmen" domainObjectName="Departmen"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" selectByExampleQueryId="false"></table>

    </context>
</generatorConfiguration>



